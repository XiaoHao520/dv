.header{
  height: 360rpx;
  width: 100%;
  background-size: cover;
  position: relative;
  margin-top: -1rpx;  
  overflow: hidden;
  background-color: #73CFFE;
}
.header_back{
  width: 110%;
  height: 360rpx;
  position: absolute;
  top: 0rpx;
  left: -5%;
  bottom: 0rpx;
  right: 0rpx;
  border-bottom-left-radius: 25%;
  border-bottom-right-radius: 25%;
}
.logs_footer{
  font-size: 28rpx;
  text-align: center;
  color: #ccc;
  height: 40rpx;
  width: 100%;
  line-height: 40rpx;
  background: orange;
}
.footer{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  text-align: center;
  color: #999;
  margin-bottom: 60rpx;
}
.footer text{
  display: inline-block;
  vertical-align: top;
  text-indent: 10rpx;
}
.footer image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 3rpx;
  /* display: block; */
}
.header_set{
  height: 50rpx;
  width: 50rpx;
  position: absolute;
  top: 0rpx;
  right: 30rpx;
  padding: 30rpx 30rpx 0rpx;
}
.header_set>image{
  width: 50rpx;
  height: 50rpx;
}
.header_box{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  bottom: 0rpx;
  right: 0rpx;
  z-index: 3;
  padding: 60rpx 30rpx 20rpx;
}
.picture{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.header_name{
  font-size: 28rpx;
  text-align: center;
  color: white;
  margin-top: 20rpx;
}
.login-form{
width:65%;
background-color:white;
height:239px;
margin:0 auto;
border-radius:7px;
top:30%;
position:relative;
}

.login-button{
width:40%;
height:30px;
line-height:30px;
border-radius:30px;
border:solid #B2B2B2 1rpx;
background-color: white;
margin-top:27px;
}

.show{
    display: block;  
}

.hide{
  display: none;
}

.login-button::after{
  border: none;
}

.list{  
  height: 172rpx;
  position: relative;
}
.list_content{
width:100%;
height:100%;
position:absolute;
z-index:5;
top:0rpx;
left:0rpx;
background-color:white;
border-bottom:solid #f0f0f0 1rpx;
}
.list_alllist{color: #999;}
.list_top{border-bottom: 1rpx solid #eee;padding: 20rpx;font-size: 28rpx;}
.list_alltext{color: #333;margin-left: 20rpx;}
.list_all{width: 300rpx;}
.list_allimg{width: 50rpx;height: 50rpx;}
.banner{padding: 0rpx;}
.list_right{width: 20rpx;height: 30rpx;margin-left: 10rpx;}
.list_fukuan{padding: 30rpx 30rpx 0rpx;}
.list_fukuanimg{width: 64rpx;height: 56rpx;margin-bottom: 20rpx;}
.list_fukuanimg2{width: 52rpx;height: 64rpx;margin-bottom: 12rpx;}
.list_fukuanimg3{width: 49rpx;height: 56rpx;margin-bottom: 20rpx;}
.list_fukuanimg4{width: 56rpx;height: 56rpx;margin-bottom: 20rpx;}
.list_collet{font-size: 26rpx;color: #333;}
.banner{
  background-color: white;
  overflow: hidden; 
}
.banner_list{
  padding: 15rpx 30rpx;
  font-size: 28rpx;
  border-bottom: 1rpx solid #eee;
}
.banner_text{
  font-size: 26rpx;
  color: #999;
}




